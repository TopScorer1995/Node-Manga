<div class="modal fade" id="realase_modal" tabindex="-1" role="dialog" aria-labelledby="exampleModalPreviewLabel" aria-hidden="true">
  
  
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-body">
        <table id="dtBasicExample" class="text-center table table-striped table-bordered" cellspacing="0" width="100%">
  <thead>
    <tr>
      <th class="th-sm">السلسلة
      </th>
      <th class="th-sm">الفصل
      </th>
      <th class="th-sm">بواسطة
      </th>
      <th class="th-sm">التوقيت
      </th>
    </tr>
  </thead>
  <tbody>
    <%data.forEach(function(chapter,index){%>
     
     <tr>

      <td class="hover_underline w_600 pointer"  onclick="location.href='/manga/<%=chapter.series_number%>'"><%=chapter.series_name%></td>
      <td class="pointer hover_underline w_600" onclick="location.href='/reader/<%=chapter.id%>'"><%=chapter.chapter_number%></td>
      <td class="hover_underline w_600 pointer"  onclick="location.href='/team/<%=chapter.by_team%>'"><%=chapter.team_name%></td>
      <td class="timeago_last_realsae" date="<%=chapter.add_date%>"><%=index%></td>
    </tr>

    <%})%>
    
  </tbody>
  <tfoot>
    <tr>
      <th>السلسلة
      </th>
      <th>الفصل
      </th>
      <th>بواسطة
      </th>
      <th>التوقيت
      </th>
    </tr>
  </tfoot>
</table>
      </div>
    </div>
  </div>
  <script type="text/javascript">
  
  $('#dtBasicExample').DataTable();
  $('.dataTables_length').addClass('bs-select');
 setInterval(function(){ $(".timeago_last_realsae").each(function(i,obj){$(this).html(jQuery.timeago(new Date($(this).attr("date"))));}) }, 500);

</script>
</div>

